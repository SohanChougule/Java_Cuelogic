<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
        
        <bean id="Vehicle" class="com.maven.ProjectName.Car"></bean> 
        <bean id="Tyre" class="com.maven.ProjectName.Tyre">
      <!--   	<property name="brand" value="MRF"></property> -->
        	<constructor-arg value="Ceat"></constructor-arg>
        </bean> 
        
        <bean id="Seat" class="com.maven.ProjectName.Seat"></bean>
        
        <context:component-scan base-package="com.maven.ProjectName"></context:component-scan>

</beans>